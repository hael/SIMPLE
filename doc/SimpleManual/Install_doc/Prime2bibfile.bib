	Mesh = {Algorithms; Capsid; Cryoelectron Microscopy; Data Interpretation, Statistical; Fourier Analysis; Imaging, Three-Dimensional; Kinesin; Models, Molecular; Papillomaviridae; Protein Structure, Quaternary; Protein Structure, Tertiary; Signal-To-Noise Ratio; Software},
	Month = {Oct},
