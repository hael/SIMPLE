	Doi = {10.1016/j.jsb.2009.06.011},
	Journal = {J Struct Biol},
	Journal-Full = {Journal of structural biology},
	Mesh = {Adenoviridae; Algorithms; Antigens, Viral, Tumor; Artificial Intelligence; Imaging, Three-Dimensional; Microscopy, Electron; Particle Size; Replication Protein A},
