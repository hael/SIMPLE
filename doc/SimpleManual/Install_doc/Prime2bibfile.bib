@article{Lander:2009aa,
	Abstract = {The use of cryoEM and three-dimensional image reconstruction is becoming increasingly common. Our vision for this technique is to provide a straightforward manner in which users can proceed from raw data to a reliable 3D reconstruction through a pipeline that both facilitates management of the processing steps and makes the results at each step more transparent. Tightly integrated with a relational SQL database, Appion is a modular and transparent pipeline that extends existing software applications and procedures. The user manages and controls the software modules via web-based forms, and all results are similarly available using web-based viewers directly linked to the underlying database, enabling even naive users to quickly deduce the quality of their results. The Appion API was designed with the principle that applications should be compatible with a broad range of specimens and that libraries and routines are modular and extensible. Presented here is a description of the design and architecture of the working Appion pipeline prototype and some results of its use.},
	Author = {Lander, Gabriel C and Stagg, Scott M and Voss, Neil R and Cheng, Anchi and Fellmann, Denis and Pulokas, James and Yoshioka, Craig and Irving, Christopher and Mulder, Anke and Lau, Pick-Wei and Lyumkis, Dmitry and Potter, Clinton S and Carragher, Bridget},
