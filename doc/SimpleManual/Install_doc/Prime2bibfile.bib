	Journal-Full = {Journal of structural biology},
	Mesh = {Amelogenin; Calcium; Emulsions; Microscopy, Atomic Force; Microscopy, Electron; Nanotubes, Carbon; Oils; Phosphates; Water},
	Month = {Apr},
	Number = {1},
	Pages = {203-12},
	Pmc = {PMC3204882},
