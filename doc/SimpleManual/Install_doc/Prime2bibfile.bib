	Mesh = {Algorithms; Animals; Brain; Drosophila melanogaster; Humans; Image Processing, Computer-Assisted; Macaca fascicularis; Mammary Glands, Human; Microscopy, Electron, Transmission; Reproducibility of Results; Time Factors},
	Month = {Oct},
