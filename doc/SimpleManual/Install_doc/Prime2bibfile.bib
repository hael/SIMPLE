	Journal = {Structure},
	Journal-Full = {Structure (London, England : 1993)},
	Mesh = {Adenosine Diphosphate; Adenosine Triphosphatases; Binding Sites; Cell Cycle Proteins; Crystallography, X-Ray; Escherichia coli; Hydrolysis; Models, Molecular; Nucleotides; Protein Binding; Protein Conformation; Protein Folding; Scattering, Small Angle; Spectrum Analysis, Raman; X-Ray Diffraction},
	Month = {May},
