	Mesh = {},
	Month = {Jun},
	Number = {},
	Pages = {584-90},
	Pmid = {},
	Pst = {ppublish},
	Title = {Electron counting and beam-induced motion correction enable near-atomic-resolution single-particle cryo-em},
	Volume = {10},
