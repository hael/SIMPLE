	Pmid = {24094406},
	Pst = {ppublish},
	Title = {Positive and negative design for nonconsensus protein-DNA binding affinity in the vicinity of functional binding sites},
	Volume = {105},
	Year = {2013},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.bpj.2013.08.033}}

@article{Ogryzko:1998aa,
	Abstract = {PCAF histone acetylase plays a role in regulation of transcription, cell cycle progression, and differentiation. Here, we show that PCAF is found in a complex consisting of more than 20 distinct polypeptides. Strikingly, some polypeptides are identical to TBP-associated factors (TAFs), which are subunits of TFIID. Like TFIID, histone fold-containing factors are present within the PCAF complex. The histone H3- and H2B-like subunits within the PCAF complex are identical to those within TFIID, namely, hTAF(II)31 and hTAF(II)20/15, respectively. The PCAF complex has a novel histone H4-like subunit with similarity to hTAF(II)80 that interacts with the histone H3-like domain of hTAF(II)31. Moreover, the PCAF complex has a novel subunit with WD40 repeats having a similarity to hTAF(II)100.},
