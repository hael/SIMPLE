	Mesh = {Amelogenin; Emulsions; Humans; Hydrogen-Ion Concentration; Hydrophobic and Hydrophilic Interactions; Micelles; Microscopy, Atomic Force; Microscopy, Electron; Nanospheres; Nanotubes; Oils; Protein Multimerization; Protein Structure, Tertiary; Recombinant Proteins; Scattering, Radiation; Water},
	Month = {Dec},
