	Journal = {J Am Chem Soc},
	Journal-Full = {Journal of the American Chemical Society},
	Mesh = {Amino Acid Sequence; Amyloid; Fungal Proteins; Models, Molecular; Nuclear Magnetic Resonance, Biomolecular; Prions; Yeasts},
	Month = {Sep},
	Number = {37},
	Pages = {14686-98},
	Pmc = {PMC3190136},
