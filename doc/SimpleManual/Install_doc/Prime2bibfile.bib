	Mesh = {Cryoelectron Microscopy; Humans; Mass Spectrometry; Mediator Complex; Models, Molecular; Multiprotein Complexes; Protein Conformation; Protein Subunits; RNA Polymerase II; Transcription Factors, TFII},
	Month = {Mar},
