	Journal-Full = {Biochimica et biophysica acta},
	Mesh = {Eukaryota; Humans; Proteasome Endopeptidase Complex; Saccharomyces cerevisiae Proteins; Trans-Activators; Transcription Factor TFIID; Transcription, Genetic; Transcriptional Activation},
	Month = {Feb},
	Number = {2},
	Pages = {97-108},
	Pmc = {PMC3018551},
	Pmid = {20800707},
	Pst = {ppublish},
	Title = {Distinct regulatory mechanisms of eukaryotic transcriptional activation by SAGA and TFIID},
	Volume = {1809},
