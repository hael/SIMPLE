	Pages = {6215-42},
	Pmid = {20885021},
	Pst = {ppublish},
	Title = {Non-rigid consistent registration of 2D image sequences},
	Volume = {55},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1088/0031-9155/55/20/012}}

@article{Sorzano:2013aa,
	Abstract = {In this chapter we describe the steps needed for reconstructing the three-dimensional structure of a macromolecular complex starting from its projections collected in electron micrographs. The concepts are shown through the use of Xmipp 3.0, a software suite specifically designed for the image processing of biological structures imaged with electron or X-ray microscopy. We illustrate the image processing workflow by applying it to the images of Bovine Papilloma virus published in Wolf et al. (Proc Natl Acad Sci USA 107:6298-6303, 2010). We show that in the case of high-quality, homogeneous datasets with a priori knowledge about the initial volume, we can have a high-resolution 3D reconstruction in less than 1 day using a computer cluster with only 32 processors.},
	Author = {Sorzano, Carlos Oscar and de la Rosa Trev{\'\i}n, J M and Ot{\'o}n, J and Vega, J J and Cuenca, J and Zald{\'\i}var-Peraza, A and G{\'o}mez-Blanco, J and Vargas, J and Quintana, A and Marabini, Roberto and Carazo, Jos{\'e} Mar{\'\i}a},
	Date-Added = {2014-02-21 19:09:55 +0000},
