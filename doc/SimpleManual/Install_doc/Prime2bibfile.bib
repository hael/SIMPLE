	Pages = {229-45},
	Pmid = {15313620},
	Pst = {ppublish},
	Title = {Exploring the structural dynamics of the E.coli chaperonin GroEL using translation-libration-screw crystallographic refinement of intermediate states},
	Volume = {342},
