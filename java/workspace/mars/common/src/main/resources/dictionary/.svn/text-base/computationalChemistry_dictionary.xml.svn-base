      &lt;/list&gt;
      &lt;list dictRef="compchem:basisSetContraction" id="basisSetContraction"&gt;
        &lt;scalar dataType="xsd:string" dictRef="compchem:basisSetShell"&gt;p&lt;/scalar&gt;
