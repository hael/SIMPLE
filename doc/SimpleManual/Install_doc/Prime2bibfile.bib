	Mesh = {Acute Disease; Brain Ischemia; Double-Blind Method; Fibrinolytic Agents; Humans; Prospective Studies; Sample Size; Stroke; Thrombolytic Therapy; Tissue Plasminogen Activator},
	Pages = {252},
	Pmc = {PMC3286387},
	Pmid = {22129158},
	Pst = {epublish},
	Title = {Update on the third international stroke trial (IST-3) of thrombolysis for acute ischaemic stroke and baseline features of the 3035 patients recruited},
