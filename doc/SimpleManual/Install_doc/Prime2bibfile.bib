	Keywords = {Camera performance; Modulation transfer function; Noise},
	Month = {Oct},
	Pages = {1-7},
	Pmc = {PMC3841286},
	Pmid = {23747527},
	Pst = {ppublish},
	Title = {Ranking TEM cameras by their response to electron shot noise},
