@article{Hohn:2007aa,
	Abstract = {SPARX (single particle analysis for resolution extension) is a new image processing environment with a particular emphasis on transmission electron microscopy (TEM) structure determination. It includes a graphical user interface that provides a complete graphical programming environment with a novel data/process-flow infrastructure, an extensive library of Python scripts that perform specific TEM-related computational tasks, and a core library of fundamental C++ image processing functions. In addition, SPARX relies on the EMAN2 library and cctbx, the open-source computational crystallography library from PHENIX. The design of the system is such that future inclusion of other image processing libraries is a straightforward task. The SPARX infrastructure intelligently handles retention of intermediate values, even those inside programming structures such as loops and function calls. SPARX and all dependencies are free for academic use and available with complete source.},
	Author = {Hohn, Michael and Tang, Grant and Goodyear, Grant and Baldwin, P R and Huang, Zhong and Penczek, Pawel A and Yang, Chao and Glaeser, Robert M and Adams, Paul D and Ludtke, Steven J},
	Date-Added = {2014-02-15 00:21:51 +0000},
