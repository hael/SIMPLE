	Doi = {10.1073/pnas.1200939109},
	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Mesh = {Kinetics; Nucleotides; RNA Polymerase II; Saccharomyces cerevisiae; Transcription, Genetic},
	Month = {Apr},
	Number = {17},
	Pages = {6555-60},
	Pmc = {PMC3340090},
	Pmid = {22493230},
	Pst = {ppublish},
	Title = {Trigger loop dynamics mediate the balance between the transcriptional fidelity and speed of RNA polymerase II},
	Volume = {109},
