	Pages = {25715-26},
	Pmid = {11337503},
	Pst = {ppublish},
	Title = {Requirement for yeast TAF145 function in transcriptional activation of the RPS5 promoter that depends on both core promoter structure and upstream activating sequences},
	Volume = {276},
