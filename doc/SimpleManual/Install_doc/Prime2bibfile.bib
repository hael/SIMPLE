	Mesh = {Chaperonin 60; Clinical Laboratory Techniques; Cryoelectron Microscopy; Image Processing, Computer-Assisted; Imaging, Three-Dimensional; Information Storage and Retrieval; Proteins; Research Design},
	Month = {Jul},
	Number = {1},
	Pages = {29-39},
