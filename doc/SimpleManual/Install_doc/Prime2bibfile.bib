	Journal = {J Mol Biol},
	Journal-Full = {Journal of molecular biology},
	Mesh = {Cryoelectron Microscopy; Hydrogen Bonding; Models, Biological; Models, Molecular; Protein Structure, Secondary; RNA, Viral; Reproducibility of Results; Static Electricity; Tobacco Mosaic Virus; Viral Proteins; Virion},
	Month = {Aug},
