	Mesh = {Adenosine Triphosphatases; Adenosine Triphosphate; Amino Acid Motifs; Amino Acid Sequence; Bacterial Proteins; Binding Sites; Crystallization; Integrins; Lyases; Magnesium; Microscopy, Electron; Models, Molecular; Molecular Sequence Data; Mutagenesis, Site-Directed; Protein Binding; Protein Structure, Quaternary; Protein Structure, Tertiary; Protein Subunits; Rhodobacter capsulatus; Sequence Alignment; Static Electricity},
	Month = {Aug},
