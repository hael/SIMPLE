	Mesh = {Algorithms; Bacteria; Bacterial Proteins; Bayes Theorem; Cryoelectron Microscopy; Image Processing, Computer-Assisted; Imaging, Three-Dimensional; Protein Conformation; Software; Viral Proteins; Viruses},
