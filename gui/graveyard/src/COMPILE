# Compile libjpeg
cd libjpeg/jpeg-9b
make clean
./configure --prefix=<install_dir>/libjpeg
make
make install

# Compile gui
cd <install_dir>
g++ simple_gui.cpp ext/mongoose/mongoose.c ext/libjpeg/lib/libjpeg.a -O3 -pthread -DSIMPLE_DIR=~/Code/SIMPLE_GIT/SIMPLE3.0/ -lsqlite3 -o simple -I ext/libjpeg/include/ -std=c++11
