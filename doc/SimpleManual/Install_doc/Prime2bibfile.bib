	Mesh = {Adenosine Triphosphate; Archaeal Proteins; Base Sequence; Cloning, Molecular; Crystallography, X-Ray; DNA Helicases; DNA Primers; Hydrolysis; Methanobacterium; Microscopy, Electron; Models, Molecular; Mutation},
	Month = {Dec},
	Number = {51},
